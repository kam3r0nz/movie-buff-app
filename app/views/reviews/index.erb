<h3>Your Reviews:</h3>
<% @reviews.each do |review| %>
    <p>Name: <%= review.user.username %></p>
    <p>Date: <%= review.date %></p>
    <p>Rating: <%= review.rating %>/5</p>
    <p><%= review.content %></p>
    <form action="/reviews/<%= review.id %>" method="GET">
        <input type="submit" value="View">
    </form>

    
    <br><br>
<% end %>

